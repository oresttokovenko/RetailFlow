{
    "family": "airbyte-task-definition",
    "containerDefinitions": [
      {
        "name": "airbyte-container",
        "image": "airbyte/server:latest",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8000,
            "hostPort": 8000
          }
        ],
        "memory": 2048,
        "cpu": 1024,
        "environment": [
          {
            "name": "DATABASE_URL",
            "value": "postgres://username:password@hostname:5432/dbname"
          }
        ]
      }
    ],
    "requiresCompatibilities": ["EC2"],
    "networkMode": "awsvpc",
    "cpu": "1024",
    "memory": "2GB"
  }
  